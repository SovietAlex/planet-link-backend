{
  "Api": {
    "DatabaseConfiguration": {
      "Server": "planet-link",
      "Name": "library",
      "Location": "US East 2"
    },
    "EndpointConfiguration": {
      "Name": "default",
      "Pattern": "{controller}/{action}/{id?}"
    },
    "AuthenticationConfiguration": {
      "Scheme": "Authentication",
      "ClaimIssuer": "Api",
      "AuthenticateUrlSegment": "/User/Authenticate"
    },
    "CrossOriginRequestConfiguration": {
      "Name": "Api",
      "Headers": [ "api-userTypeId", "api-timezoneId", "api-token", "api-code", "api-page" ],
      "Methods": [ "GET", "POST", "PUT", "DELETE" ]
    },
    "SwaggerConfiguration": {
      "Path": "/swagger",
      "Title": "Api"
    }
  },
  "Api.Library": {
    "UserConfiguration": {
      "Limit": {
        "SearchStocktwitsLimit": 6
      },
      "Duration": {
        "StocktwitsTokenDurationInHours": 1
      },
      "Threshold": {
        "GoogleUpdateThresholdInHours": 1,
        "StocktwitsUpdateThresholdInHours": 1
      }
    }
  }
}